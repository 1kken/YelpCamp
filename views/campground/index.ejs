<% layout('layouts/boilerplate') -%>
    <h1>Camp grounds</h1>
    <div>
        <a href="campground/new">Add new Camp Ground</a>
    </div>
    <%for(let data of datas){%>
        <div class="row my-3">
            <div class="col-md-4">
                <img src="<%= data.image %>" alt="camp image" class="img-fluid">
            </div>
            <div class="col-md-8">
                <div class="card-body">
                    <h5 class="card-title"><%=data.title%></h5>
                    <p class="card-text"><%=data.description%></p>
                    <small class="text-muted"><%=data.location%> </small>
                    <a href="/campground/<%=data.id%>" class="btn btn-primary">Show details</a>
                </div>
            </div>
        </div>
    <%}%>

